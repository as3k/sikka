{
    "name": "sikka",
    "description": "A custom theme for ghost",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=4.0.0",
        "ghost-api": "v4"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan .",
        "test:ci": "gscan --fatal --verbose .",
        "pretest": "gulp build",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
    },
    "author": {
        "name": "Zachary GUerrero",
        "email": "hello@zkg.io",
        "url": "https://zkg.io"
    },
    "keywords": [
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/as3k/sikka"
    },
    "devDependencies": {
        "autoprefixer": "10.2.5",
        "cssnano": "4.1.10",
        "gscan": "4.0.1",
        "gulp": "4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-livereload": "4.0.2",
        "gulp-postcss": ">=9.0.0",
        "gulp-sourcemaps": "3.0.0",
        "gulp-uglify": "3.0.2",
        "gulp-util": "3.0.8",
        "gulp-watch": "5.0.1",
        "gulp-zip": "5.1.0",
        "postcss": "^8.2.8",
        "postcss-color-mod-function": "3.0.3",
        "postcss-easy-import": "3.0.0",
        "tailwind-nord": "^1.2.0",
        "tailwindcss": "^2.0.4"
    },
    "browserslist": [
        "defaults"
    ],
    "config": {
        "posts_per_page": 15,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1200
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {}
}
